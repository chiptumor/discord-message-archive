{
	title: "Title of archive",
	// Sets the title of the page
	// Default is "Discord Message Archive"
	config: {
	// Customizes aspects of the archive or display
	// Use defaults if falsey
	/// Currently not in use!
	},
	user: [
	// Defines users to be displayed in the users section and conversation
	// Mandatory for every user involved
		{
			reference: "user",
			// Utilized in user specifications for better readability
			// Default is index position
			id: 123,
			// Utilized in message contents replace mentions with the nickname of the user under the ID
			// Example: "<@123>" is replaced with "@Archived nickname"
			available: {
				status: false,
				// Determines if the user should have an unavailability warning
				reason: "Reason for unavailability"
				// Determines the reason specified for unavailability
				// Ignore if falsey or if 'status' is true
			},
			archive: {
			// Data to be used in author displays in users section and over messages
			// Use defaults if falsey
				avatar: "https://chiptumor.github.io/discord-message-archive/template/file/avatar/archive.png",
				// Default is "https://cdn.discordapp.com/embed/avatars/0.png"
				user: "Archived username",
				// Default is "???"
				display: "Archived nickname",
				// Default is user
				pronouns: "Archived pronouns",
				// Ignore if falsey
				bio: "Archived about me",
				// Ignore if falsey
				roles: [
				// Ignore if falsey
					{
						name: "Archived name of role",
						// Mandatory
						color: "#123DEF"
						// Default is "#99ABB4" (TODO)
					}
				]
			},
			recent: {
			// Data to be displayed under archived info
			// Ignore if null
				/* FOR EACH KEY:
				 * Inherit from (..).archive if true
				 * Ignore if null or false */
				avatar: "https://chiptumor.github.io/discord-message-archive/template/file/avatar/recent.png",
				user: "Current username",
				display: "Current nickname",
				pronouns: "Current pronouns",
				bio: "Current about me",
				roles: [
					{
						name: "Current name of role",
						color: "#789ABC"
					}
				]
			}
		}
	],
	content: [
	// Defines conversation to be displayed
		{
			type: 0, // MEANS MESSAGE
			// Determines how this content is displayed
			// Default is 0
			reply: {
			// Displays message being replied to
			// Inherit from message ID map if number
			// Ignore if null
			/* FOR EACH KEY:
			 * Inherit from (TODO)
			 * */
				avatar: "https://chiptumor.github.io/discord-message-archive/template/file/avatar/archive.png"
			},
			author: "user",
			messages: [
				{
					id: 456,
					highlight: 0,
					displayed: "Displayed message",
					original: "Original message",
					timestamp: {
						short: "Short date",
						long: "Long date",
						time: 0
					}
				},
				{
					id: 457,
					highlight: 1,
					displayed: "Displayed message",
					original: "Original message",
					timestamp: {
						short: "Short date",
						long: "Long date",
						time: 0
					},
					attachment: [
						{
							type: 0,
							filename: "filename.ext",
							size: "File size",
							url: [
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-file.ext",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-file.ext"
							]
						},
						{
							type: 1,
							filename: "image.png",
							size: "File size",
							url: [
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-image.png",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-image.png"
							]
						},
						{
							type: 2,
							filename: "video.mp4",
							size: "File size",
							url: [
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-video.mp4",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-video.mp4"
							]
						},
						{
							type: 3,
							filename: "audio.mp3",
							size: "File size",
							url: [
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-audio.mp3",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-audio.mp3"
							]
						}
					],
					animation: {
						wait: 0,
						type: 0
					}
				}
			]
		},
		{
			author: "date",
			color: 0,
			content: "Datestamp text"
		},
		{
			author: "flag",
			color: 0,
			content: "Flag text"
		}
	]
}
