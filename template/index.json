{
	title: "Title of archive",
	// Sets the title of the page
	// Default is "Discord Message Archive"
	config: {
	// Customizes aspects of the archive or display
	// Use defaults if falsey
	/// Currently not in use!
	},
	user: [
	// Defines users to be displayed in the users section and conversation
	// Ignore if null
		{
			reference: "user",
			// Utilized in user specifications for better readability
			// Default is stringified index position
			id: 123,
			// Utilized in message contents replace mentions with the nickname of the user under the ID
			// Example: "<@123>" is replaced with "@Archived nickname"
			bot: false,
			// 
			archive: {
				// Data to be used in author displays in users section and over messages
				// Use defaults if falsey
				avatar: "https://chiptumor.github.io/discord-message-archive/template/file/user/archive.png",
				// Default is "https://cdn.discordapp.com/embed/avatars/0.png"
				user: "Archived username",
				// Default is "???"
				display: "Archived nickname",
				// Default is user
				pronouns: "Archived pronouns",
				// Ignore if falsey
				bio: "Archived about me",
				// Ignore if falsey
				roles: {
				// Use defaults if falsey
					preview: {
						icon: "https://chiptumor.github.io/discord-message-archive/template/file/user/role-preview.png",
						// Ignore if falsey
						color: "#456789"
						// Default is "#FFFFFF"
					},
					list: [
					// Ignore if falsey
						{
							name: "Archived name of role",
							// Mandatory
							icon: "https://chiptumor.github.io/discord-message-archive/template/file/user/role-archive.png",
							// Ignore if falsey
							color: "#123DEF"
							// Default is "#99ABB4" (TODO)
						}
					]
				}
			},
			recent: {
				// Data to be displayed under archived info
				// Ignore if null
				/* FOR EACH KEY:
				 * Inherit from (..).archive if true
				 * Ignore if null or false */
				avatar: "https://chiptumor.github.io/discord-message-archive/template/file/user/recent.png",
				user: "Current username",
				display: "Current nickname",
				pronouns: "Current pronouns",
				bio: "Current about me",
				roles: [
					{
						name: "Current name of role",
						color: "#789ABC"
					}
				]
			},
			available: {
				status: false,
				// Determines if the user should have an unavailability warning
				reason: "Reason for unavailability"
				// Determines the reason specified for unavailability
				// Ignore if falsey or if 'status' is true
			}
		}
	],
	content: [
	// Defines conversation to be displayed
		{
			type: 0, // MEANS MESSAGE
			// Determines how this content is displayed
			// Default is 0
			reply: {
			// Displays message being replied to
			// Inherit from message ID if number
			// Ignore if falsey
				deleted: false,
				// Determines whether reply is deleted
				// Default is false
				id: 234,
				// Determines what message ID to jump to when clicked
				// Ignore if falsey
				avatar: "https://chiptumor.github.io/discord-message-archive/template/file/avatar/reply.png",
				// Inherit from referenced user or use as URL if string
				// Inherit from user at index or message ID or user ID if number
				// "https://cdn.discordapp.com/embed/avatars/0.png" if falsey and deleted is true
				// Ignore if falsey
				alias: "chiptumor",
				// Inherit from referenced user or use as text if string
				// Inherit from user at index or message ID or user ID if number
				// Ignore if falsey
				displayed: "Displayed reply",
				// Use as text if string
				// Inherit from message ID if number
				// Ignore if falsey
				original: "Original reply"
				// Use as text if string
				// Inherit from message ID if number
				// "*Deleted message*" if falsey and deleted is true
				// Ignore if falsey
			},
			author: "user",
			// Inherit from referenced user if string
			// Inherit from user at index or user ID if number
			// Can be set directly as user object except reference key
			messages: [
			// Collection of messages to be displayed in message group
				{
					id: 456,
					// Utilized in replies
					// Ignore if falsey
					highlight: 0,
					// Determines how the message should be highlighted
					// Default is 0
					/* LEGEND:
					 * 0 none
					 * 1 mention
					 * 2 ephemeral
					 * 3 danger
					 * 4 failure */
					displayed: "Displayed message",
					// Message to be displayed by default
					// Ignore if falsey
					original: "Original message",
					// Message to be displayed when original text is requested
					timestamp: {
						short: "Short date",
						// Date to be displayed besides username (first message) or to left of hovered messages
						// Ignore if falsey
						long: "Long date",
						// Date to be displayed when short dates are hovered over
						// Ignore if falsey
						time: 0
						// Snowflake time of message to display when requested
						// Ignore if falsey
					},
					attachment: [
					// Defines attachments in message
					// Ignore if falsey
						{
							type: 0,
							// Determines the type of file presented
							// Default is 0
							/* LEGEND:
							 * 0 Generic
							 * 1 Image
							 * 2 Video
							 * 3 Audio */
							name: "filename.ext",
							// Displays if generic file or audio
							// Fallback to filename from retrieved file if falsey
							size: "File size",
							// Displays under filename if generic or audio
							// Fallback to file size from retrieved file if falsey
							url: [
							// URL to use for attachment
							// Multiple allowed for use as fallbacks
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-file.ext",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-file.ext"
							]
						},
						/// FURTHER ATTACHMENTS FOR STYLE TESTING; IGNORE (
						{
							type: 1,
							filename: "image.png",
							size: "File size",
							url: [
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-image.png",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-image.png"
							]
						},
						{
							type: 2,
							filename: "video.mp4",
							size: "File size",
							url: [
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-video.mp4",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-video.mp4"
							]
						},
						{
							type: 3,
							filename: "audio.mp3",
							size: "File size",
							url: [
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/main-audio.mp3",
								"https://chiptumor.github.io/discord-message-archive/template/file/attachment/fallback-audio.mp3"
							]
						}
					],
					// )
					animation: {
					// Events to play for this message when animation is played
					/// Not currently handled!
						wait: 0,
						// Amount of time before typing animation is triggered
						// Default is 0
						type: 0
						// Amount of time that the user seemingly takes to type before this message is sent
						// Default is 0
					}
				}
			]
		},
		{
			type: 1, // MEANS SYSTEM MESSAGE
		},
		{
			type: 2, // MEANS DATESTAMP
			color: 0,
			content: "Datestamp text"
		},
		{
			type: 3, // MEANS FLAG
			color: 0,
			content: "Flag text"
		},
		{
			type: 4, // MEANS HIDDEN MESSAGES
		},
		{
			type: 5, // MEANS COMMENT
		}
	]
}
